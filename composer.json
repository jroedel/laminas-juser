{
    "name": "jroedel/laminas-juser",
    "description": "A fork of manuakasam/SamUser. Glues together LmcUser, BjyAuthorize, SlmLocale, and GoalioRememberMe",
    "license": "MIT",
    "keywords": [
        "laminas"
    ],
    "homepage": "https://github.com/jroedel/laminas-juser",
    "require": {
        "php": "^8.0",
        "laminas/laminas-db": "^2.4.0",
        "laminas/laminas-mvc-form": "^1.0",
        "laminas/laminas-form": "^2.4.0",
        "laminas/laminas-crypt": "^2.4.0 || ^3.1.0",
        "laminas/laminas-math": "^2.7 || ^3.0",
        "kokspflanze/bjy-authorize": "^1.7",
        "jroedel/zf-snap-geoip": "^3.0",
        "zf-commons/zfc-user": "3.x@dev",
        "jroedel/locale": "dev-master@dev",
        "beaucal/beaucal-invalid-session": "^0.3.0",
        "jroedel/laminas-sion-model": "dev-master",
        "multidots/zf3-rest-api": "dev-master"
    },
	"autoload": {
    	"psr-4": {
        	"JUser\\": "module/JUser/src/"
    	}
	},
	"repositories": [
	   {
	     "type": "vcs",
	     "url": "https://github.com/basz/SlmLocale"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/jroedel/ZfSnapGeoip"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/jroedel/zf3-sion-model"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/kokspflanze/BjyAuthorize"
	   }
	]
}
