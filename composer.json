{
    "name": "jroedel/zf3-juser",
    "description": "A fork of manuakasam/SamUser. Glues together ZfcUser, BjyAuthorize, SlmLocale, and GoalioRememberMe",
    "license": "MIT",
    "keywords": [
        "framework",
        "zf3"
    ],
    "homepage": "https://github.com/jroedel/zf3-juser",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-db": "^2.4.0",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-form": "^2.4.0",
        "zendframework/zend-crypt": "^2.4.0 || ^3.1.0",
        "zendframework/zend-math": "^2.7 || ^3.0",
        "kokspflanze/bjy-authorize": "^1.7",
        "jroedel/zf-snap-geoip": "^3.0",
        "zf-commons/zfc-user": "3.x@dev",
        "jroedel/locale": "dev-master@dev",
        "beaucal/beaucal-invalid-session": "^0.3.0",
        "swiftmailer/swiftmailer": "^6.0",
        "jroedel/zf2-sion-model": "dev-master",
        "multidots/zf3-rest-api": "dev-master"
    },
	"autoload": {
    	"psr-4": {
        	"JUser\\": "module/JUser/src/"
    	}
	},
	"repositories": [
	   {
	     "type": "vcs",
	     "url": "https://github.com/basz/SlmLocale"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/jroedel/ZfSnapGeoip"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/jroedel/zf3-sion-model"
	   },
	   {
	     "type": "vcs",
	     "url": "https://github.com/kokspflanze/BjyAuthorize"
	   }
	]
}
