        "zendframework/zend-crypt": "^2.4.0 || ^3.1.0",
        "bjyoungblood/bjy-authorize": "^1.4.0",
        "snapshotpl/zf-snap-geoip": "2.*",
    	"goalio/goalio-rememberme": "1.*",
        "zf-commons/zfc-user": "1.0.0",
        "bb-drummer/locale": "dev-master@dev"
    },
    "autoload": {
